{
  "name": "@hedgedoc/markdown-it-plugins",
  "type": "module",
  "version": "2.0.0",
  "description": "A collection of used and modified markdown-it plugins.",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "./dist/index.esm.js",
  "unpkg": "./dist/index.umd.js",
  "exports": {
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "default": "./dist/index.mjs"
  },
  "files": [
    "LICENSES/*",
    "package.json",
    "README.md",
    "dist/index.d.ts",
    "dist/image-size/index.d.ts",
    "dist/image-size/parse-image-size.d.ts",
    "dist/image-size/specialCharacters.d.ts",
    "dist/task-lists/index.d.ts",
    "dist/toc/index.d.ts",
    "dist/toc/plugin.d.ts",
    "dist/toc/toc-options.d.ts",
    "dist/toc/toc-body-renderer.d.ts",
    "dist/toc/toc-ast.d.ts",
    "dist/index.cjs.map",
    "dist/index.cjs",
    "dist/index.mjs.map",
    "dist/index.mjs",
    "dist/index.esm.js.map",
    "dist/index.esm.js",
    "dist/index.umd.js.map",
    "dist/index.umd.js"
  ],
  "scripts": {
    "test": "jest",
    "build": "microbundle",
    "prepublish": "rm -rf dist && yarn lint && yarn build && yarn test",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --fix --ext .ts src"
  },
  "keywords": [
    "markdown",
    "markdown-it",
    "image size",
    "task lists"
  ],
  "author": "The HedgeDoc developers",
  "license": "MIT",
  "peerDependencies": {
    "markdown-it": ">=12"
  },
  "devDependencies": {
    "@jest/types": "29.5.0",
    "@mrdrogdrog/optional": "1.1.0",
    "@types/markdown-it": "12.2.3",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "4.2.1",
    "html-entities": "2.3.3",
    "jest": "29.5.0",
    "markdown-it": "13.0.1",
    "microbundle": "0.15.1",
    "prettier": "2.8.8",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hedgedoc/markdown-it-plugins.git"
  },
  "bugs": {
    "url": "https://github.com/hedgedoc/markdown-it-plugins/issues"
  },
  "homepage": "https://github.com/hedgedoc/markdown-it-plugins#readme",
  "packageManager": "yarn@3.5.1"
}
